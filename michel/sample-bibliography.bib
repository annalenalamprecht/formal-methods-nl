keywords = {Behavioral analysis,Computer software,Model checking,Modular verification,Parallel composition,Product families,Reduction techniques,Semantics,Software Product Line,Software design,Variability,Variability analysis,Verification},
pages = {78--85},
title = {{Software product line analysis with mCRL2}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84907833969{\&}partnerID=40{\&}md5=25757423413a40cb47afdd9fc2e3e5fb},
