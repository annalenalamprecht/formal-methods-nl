@article{Rosenmuller2011,
abstract = {Software product lines (SPLs) and adaptive systems aim at variability to cope with changing requirements. Variability can be described in terms of features, which are central for development and configuration of SPLs. In traditional SPLs, features are bound statically before runtime. By contrast, adaptive systems support feature binding at runtime and are sometimes called dynamic SPLs (DSPLs). DSPLs are usually built from coarse-grained components, which reduces the number of possible application scenarios. To overcome this limitation, we closely integrate static binding of traditional SPLs and runtime adaptation of DSPLs. We achieve this integration by statically generating a tailor-made DSPL from a highly customizable SPL. The generated DSPL provides only the runtime variability required by a particular application scenario and the execution environment. The DSPL supports self-configuration based on coarse-grained modules. We provide a feature-based adaptation mechanism that reduces the effort of computing an optimal configuration at runtime. In a case study, we demonstrate the practicability of our approach and show that a seamless integration of static binding and runtime adaptation reduces the complexity of the adaptation process. Copyright {\textcopyright} 2011 ACM.},
author = {Rosenm{\"{u}}ller, M and Siegmund, N and Pukall, M and Apel, S},
doi = {10.1145/2047862.2047866},
file = {:D$\backslash$:/OneDrive/Documents/TUE/Graduation/Papers/Tailoring Dynamic Software Product Lines.pdf:pdf},
isbn = {9781450306898 (ISBN)},
journal = {10th ACM International Conference on Generative Programming and Component Engineering, GPCE'11},
keywords = {Adaptation mechanism,Adaptation process,Adaptive systems,Application scenario,Coarse-grained,Customizable,Dynamic binding,Dynamic softwares,Engineering,Execution environments,Feature binding,Feature-based,Featureoriented programming,Industrial engineering,Optimal configurations,Runtime adaptation,Runtimes,Seamless integration,Self configuration,Software Product Line,Software product lines},
pages = {3--12},
title = {{Tailoring dynamic software product lines}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-81455154937{\&}partnerID=40{\&}md5=47001f2e27a15b7855d19b84c0a8391b},
year = {2011}
}
