@article{Ramadge1984,
abstract = {This paper studies the control of a class of discrete event processes, i.e. processes that are discrete, asynchronous and possibly nondeter-ministic. The controlled process is described as the generator of a formal language, while the controller, or supervisor, is constructed from the grammar of a specified target language that incorporates the desired closed-loop system behavior. The existence problem for a supervisor is reduced to finding the largest controllable language contained in a given legal language. Two examples are provided. This research was partially supported by NSERC (Canada), Grant No. A-7399.},
author = {Ramadge, P and Wonham, W},
doi = {10.1007/BFb0006306},
