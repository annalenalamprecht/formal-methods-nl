keywords = {Adaptive systems,Computer software reusability,Dynamics,Network architecture,Production engineering,Software design},
number = {12},
pages = {52--68},
title = {{Towards feature-oriented variability reconfiguration in dynamic software product lines}},
url = {http://dx.doi.org/10.1007/978-3-642-21347-2{\_}5},
volume = {6727 LNCS},
year = {2011}
}
