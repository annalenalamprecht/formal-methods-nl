isbn = {0768014778},
issn = {0042-3114},
journal = {Vehicle System Dynamics},
keywords = {All-wheel-drive,Control,Torque distribution,Vehicle handling},
number = {7},
pages = {529--546},
title = {{Independent control of all-wheel-drive torque distribution}},
